{
  "name": "diraq_desktop",
  "version": "0.0.0",
  "description": "DiraQ desktop application",
  "author": "DiraQ",
  "main": "main.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "dev": "nuxt & wait-on http://localhost:3000/ && npm start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "5.3.6",
    "cross-env": "5.2.0",
    "element-ui": "2.4.11",
    "nuxt": "2.3.4"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "electron": "4.0.0",
    "eslint": "5.11.1",
    "eslint-config-prettier": "3.3.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-vue": "5.0.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "wait-on": "3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --format codeframe --fix",
      "git add"
    ]
  }
}
